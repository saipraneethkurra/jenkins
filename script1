pipeline {
    agent any
    environment {
        GIT_SSH_COMMAND = "ssh -i ~/.ssh/id_ed25519 -o StrictHostKeyChecking=no"
    }
    stages {
        stage('Clone Repository') {
            steps {
                script {
                    sh 'git clone git@github.com:saipraneethkurra/jenkins.git'
                }
            }
        }
    }
}
